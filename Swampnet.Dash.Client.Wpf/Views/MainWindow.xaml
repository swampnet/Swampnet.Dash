<Window x:Class="Swampnet.Dash.Client.Wpf.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Swampnet.Dash.Client.Wpf"
        xmlns:view="clr-namespace:Swampnet.Dash.Client.Wpf.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<view:DashboardView DataContext="{Binding Lefty}" Grid.Column="0" Margin="10"/>
		<view:DashboardView DataContext="{Binding Righty}" Grid.Column="1" Margin="10"/>
	</Grid>
</Window>
